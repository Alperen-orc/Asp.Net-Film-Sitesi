@model List<AppUser>

<h1>Kullanıcı Rol Listesi</h1>

<table class="table table-bordered">
    <tr>
        <th>Kullanıcı</th>
        <th>Rol Ata</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td><a href="/AdminRol/AssignRole/@item.Id" class="btn btn-outline-warning"></a>Rol Ata</td>
        </tr>
    }
</table>
<a href="/AdminRol/Index" class="btn btn-primary">Rol Listesine Dön</a>
